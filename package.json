{
  "name": "symptom-tracker",
  "version": "1.0.1",
  "description": "Health and fitness logger to track symptoms",
  "main": "y",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/server.js",
